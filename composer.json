{
    "name": "technoyer/laravel-cuttly",
    "description": "Helping developers using Cutt.ly API for shorter URLs",
    "keywords": ["laravel", "cuttly", "bitly", "package", "short", "url", "link", "technoyer", "short url", "laravel short link"],
    "license": "MIT",
    "homepage": "https://github.com/Technoyer/laravel-cuttly",
    "support": {
        "issues": "https://github.com/Technoyer/laravel-cuttly/issues",
        "source": "https://github.com/Technoyer/laravel-cuttly"
    },
    "autoload": {
        "psr-4": {
            "Technoyer\\Cuttly\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mohamed Gomaa",
            "email": "technoyer@gmail.com"
        }
    ],
    "extra": {
      "laravel": {
        "providers": [
          "Technoyer\\Cuttly\\CuttlyServiceProvider"
        ],
        "aliases": {
          "Bitly": "Technoyer\\Cuttly\\Facade\\Cuttly"
        }
      }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "illuminate/support": "^5.8 || ^6.0 || ^7.0 || ^8.0"
    }
}
