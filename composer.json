    "description": "Helping developers using Cutt.ly API for shorter URLs",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Technoyer\\Cuttly\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mohamed Gomaa",
            "email": "technoyer@gmail.com"
        }
